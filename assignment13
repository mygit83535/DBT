kd4_akshada_83535>select Cname,City,case when Rating <200 then 'LOW RATING' end 'Rating' from CUSTOMERS where Rating<200
    -> union
    -> select Cname,City,case when Rating>=200 then 'HIGH RATING' end from CUSTOMERS where Rating>=200;
